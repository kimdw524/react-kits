{
  "compilerOptions": {
    "paths": {
      "@/plugins/*": ["./plugins/*"],
      "@/*": ["./src/*"]
    },
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*",
    "./gatsby-config.ts",
    "./plugins/**/*",
    "gatsby-browser.tsx"
  ]
}
